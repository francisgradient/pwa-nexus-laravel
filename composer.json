{
    "name": "francisabch12/pwa-laravel",
    "description": "PWA for Laravel 12",
    "type": "service-enhancement",
    "license": "MIT",
    "authors": [
        {
            "name": "Aakashdeep S. B.",
            "email": "abchhackerz@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/pint": "^1.13",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "files": [
            "src/Pwahelpers.php"
        ],
        "psr-4": {
            "FrancisLarvelPwa\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra":{
        "laravel": {
            "providers": [
                "FrancisLarvelPwa\\PwaServiceProvider"
            ],
            "aliases": {
                "Pwahelpers": "FrancisLarvelPwa\\Facades\\Pwa"
            }
        }
    }
    
}
